<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.apisupport.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/nb-module-project/3">
            <code-name-base>org.praxislive.compiler.javac</code-name-base>
            <suite-component/>
            <module-dependencies>
                <dependency>
                    <code-name-base>net.neilcsmith.praxis.compiler</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.0</specification-version>
                    </run-dependency>
                </dependency>
            </module-dependencies>
            <friend-packages>
                <friend>net.neilcsmith.praxis.compiler</friend>
                <package>org.praxislive.compiler.source.tree</package>
                <package>org.praxislive.compiler.source.util</package>
                <package>org.praxislive.compiler.tools.javac</package>
                <package>org.praxislive.compiler.tools.javac.api</package>
                <package>org.praxislive.compiler.tools.javac.code</package>
                <package>org.praxislive.compiler.tools.javac.comp</package>
                <package>org.praxislive.compiler.tools.javac.file</package>
                <package>org.praxislive.compiler.tools.javac.jvm</package>
                <package>org.praxislive.compiler.tools.javac.main</package>
                <package>org.praxislive.compiler.tools.javac.model</package>
                <package>org.praxislive.compiler.tools.javac.nio</package>
                <package>org.praxislive.compiler.tools.javac.parser</package>
                <package>org.praxislive.compiler.tools.javac.processing</package>
                <package>org.praxislive.compiler.tools.javac.sym</package>
                <package>org.praxislive.compiler.tools.javac.tree</package>
                <package>org.praxislive.compiler.tools.javac.util</package>
            </friend-packages>
            <class-path-extension>
                <runtime-relative-path>ext/praxis-compiler-javac8.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/praxis-compiler-javac8.jar</binary-origin>
            </class-path-extension>
        </data>
    </configuration>
</project>
